rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /games/{gameId} {
      allow read: if true;
      allow write: if request.auth.uid != null;
    }
    // match /{document=**} {
    //   allow read, write: if
    //       request.time < timestamp.date(2023, 6, 10);
    // }
  }
}